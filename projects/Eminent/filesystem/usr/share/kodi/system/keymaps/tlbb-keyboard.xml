<!-- Mappings for The Little Black Box Remote -->
<keymap>
        <global>
                <keyboard>
                        <power>Shutdown</power>                                                  <!-- Button 01 : Power -->
                        <volume_mute>Mute</volume_mute>                                                                                                         <!-- Button 02 : Mute -->
                        <comma>Seek(-10)</comma>                                                                           <!-- Button 03 : SkipPrevious -->
                        <play_pause>playpause</play_pause>                                                                         <!-- Button 04 : Play/Pause -->
                        <period>Seek(10)</period>                                                                                                       <!-- Button 05 : SkipNext -->
                        <f5>RunScript(service.coreelec.settings)</f5>                                          <!-- Button 06 : WiFi -->
	    <r>Rewind</r>
                        <x>Stop</x>                                                                                                   <!-- Button 08 : Stop -->
                        <f>FastForward</f>                                                          <!-- Button 09 : FastForward -->
                        <m>ActivateWindow(PlayerControls)</m>                                                     <!-- Button 10 : Menu -->
                        <tab>Fullscreen</tab>
                        <f12>ActivateWindow(Settings)</f12>                                           <!-- Button 12 : Settings/wheel -->
                        <backspace>Back</backspace>                                                                                           <!-- Button 13 : Back -->
                        <volume_up>VolumeUp</volume_up>                                                                                                     <!-- Button 19 : Vol+ -->
                        <volume_down>VolumeDown</volume_down>                                                                                           <!-- Button 20 : Vol- -->
                        <pageup>PageUp</pageup>                                                                                         <!-- Button 21 : PG Up+ -->
                        <pagedown>PageDown</pagedown>                                                                           <!-- Button 22 : PG Down- -->
                        <browser_home>ActivateWindow(Home)</browser_home>                                                                           <!-- Button 23 : Home -->
                        <c>ContextMenu</c>                                                                                            <!-- Button 24 : Context Menu -->
                        <f6>activatewindow(music)</f6>                          <!-- Button 28 : Music -->
                        <f7>ActivateWindow(Videos,sources://video/)</f7>        <!-- Button 32 : Movies -->
                        <insert>ActivateWindow(Videos,videodb://tvshows/,return)</insert>       <!-- Button 36 : Series -->
                        <o>Info</o>                                                                                                       <!-- Button 37 : Info -->
                        <browser_back>Back</browser_back>                                                                                     <!-- Button 39 : Back -->
                        <semicolon>ActivateWindow(pictures,return)</semicolon>                                                        <!-- Button 40 : Pictures -->
                        <f11>ActivateWindow(Favourites)</f11>                                       <!-- Favorites button on IR remote -->
                        <!-- <f1>ActivateWindow(Favourites)</f1> -->
                        <f2>ActivateWindow(Videos,sources://video/)</f2>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(pictures,return)</f4>
                </keyboard>
        </global>
        <LoginScreen>
                <keyboard>
                <!-- Empty -->
                </keyboard>
        </LoginScreen>
        <Home>
                <keyboard>
                        <o>ActivateWindow(1111)</o>
                        <!--<browser_home>FullScreen</browser_home>-->
                        <c>Playlist</c>
                        <zero>Playlist</zero>
                        <f3>ActivateWindow(Favourites)</f3>
                </keyboard>
        </Home>
        <SystemInfo>
                <keyboard>
                        <o>Back</o>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </SystemInfo>
        <Programs>
                <keyboard>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </Programs>
        <Settings>
                <keyboard>
                        <c>Back</c>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                        <o>ActivateWindow(1111)</o>
                </keyboard>
        </Settings>
        <VirtualKeyboard>
                <keyboard>
                        <backspace>Backspace</backspace>
                </keyboard>
        </VirtualKeyboard>
        <FileManager>
                <keyboard>
                        <f1>Container.NextViewMode</f1>
                        <f2>Info</f2>
                        <f3>SendClick(8)</f3>
                        <f4>SendClick(3)</f4>
                </keyboard>
        </FileManager>
        <MusicPlaylist>
                <keyboard>
                        <zero>Back</zero>
                        <pageup>MoveItemUp</pageup>
                        <pagedown>MoveItemDown</pagedown>
                        <f1>Container.NextViewMode</f1>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                        <o>Info</o>
                </keyboard>
        </MusicPlaylist>
        <MusicPlaylistEditor>
                <keyboard>
                        <pageup>MoveItemUp</pageup>
                        <pagedown>MoveItemDown</pagedown>
                        <f1>Delete</f1>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </MusicPlaylistEditor>
        <Music>
                <keyboard>
                        <zero>ActivateWindow(MusicPlaylist)</zero>
                        <f1>Container.NextViewMode</f1>
                        <f2>Info</f2>
                        <f3>SendClick(8)</f3>
                        <f4>SendClick(3)</f4>
                        <o>Info</o>
                </keyboard>
        </Music>
        <FullscreenVideo>
                <keyboard>
                        <f12>ActivateWindow(OSDVideoSettings)</f12>
                        <pageup>subtitledelayplus</pageup>
                        <pagedown>subtitledelayminus</pagedown>
                        <f>FastForward</f>
                        <r>Rewind</r>
                        <backspace>Fullscreen</backspace>
                        <return>OSD</return>
                        <m>OSD</m>
                        <o>CodecInfo</o>
                        <left>StepBack</left>
                        <right>StepForward</right>
                        <up>BigStepForward</up>
                        <down>BigStepBack</down>
                        <f6>AudioDelay</f6>
                        <semicolon>AspectRatio</semicolon>
                        <f1>ActivateWindow(SubtitleSearch)</f1>
                        <f2>ActivateWindow(OSDVideoSettings)</f2>
                        <f3>ActivateWindow(OSDAudioSettings)</f3>
                        <f4>ActivateWindow(VideoBookmarks)</f4>
                        <c>ActivateWindow(VideoPlaylist)</c>
                        <period>Seek(10)</period>
                        <comma>Seek(-10)</comma>
                </keyboard>
        </FullscreenVideo>
        <VideoTimeSeek>
                <keyboard>
                        <return>Select</return>
                </keyboard>
        </VideoTimeSeek>
        <FullscreenInfo>
                <keyboard>
                        <f>Seek(10)</f>
                        <r>Seek(-10)</r>
                        <o>Back</o>
                        <m>OSD</m>
                </keyboard>
        </FullscreenInfo>
        <PlayerControls>
                <keyboard>
                        <m>Back</m>
                </keyboard>
        </PlayerControls>
        <Visualisation>
                <keyboard>
                        <c>ActivateWindow(1112)</c>
                        <c>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"ActivateWindow(PVROSDChannels)","ActivateWindow(MusicPlaylist)")</c>
                        <f>FastForward</f>
                        <r>Rewind</r>
                        <backspace>Fullscreen</backspace>
                        <return>OSD</return>
                        <m>OSD</m>
                        <o>Info</o>
                        <left>SkipPrevious</left>
                        <right>SkipNext</right>
                        <up>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"ActivateWindow(PVROSDChannels)","Action(IncreaseRating)")</up>
                        <down>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"ActivateWindow(PVROSDChannels)","Action(DecreaseRating)")</down>
                        <f1>RunScript(script.cu.lrclyrics)</f1>
                        <f2>RunScript(special://xbmc/system/keymaps/tlbb/py_activate_click.py,500)</f2>
                        <f3>Addon.Default.OpenSettings(xbmc.player.musicviz)</f3>
                        <f4>ActivateWindow(VisualisationPresetList)</f4>
                        <c>ActivateWindow(MusicPlaylist)</c>
                </keyboard>
        </Visualisation>
        <MusicOSD>
                <keyboard>
                        <c>ActivateWindow(1112)</c>
                        <f>FastForward</f>
                        <r>Rewind</r>
                        <period>SkipNext</period>
                        <comma>SkipPrevious</comma>
                        <m>Back</m>
                        <o>CodecInfo</o>
                        <f1>RunScript(script.cu.lrclyrics)</f1>
                        <f2>SendClick(500)</f2>
                        <f3>Addon.Default.OpenSettings(xbmc.player.musicviz)</f3>
                        <f4>ActivateWindow(VisualisationPresetList)</f4>
                        <c>ActivateWindow(MusicPlaylist)</c>
                </keyboard>
        </MusicOSD>
        <VisualisationPresetList>
                <keyboard>
                        <f>FastForward</f>
                        <r>Rewind</r>
                        <period>SkipNext</period>
                        <comma>SkipPrevious</comma>
                        <m>Back</m>
                        <o>CodecInfo</o>
                        <f4>Close</f4>
                        <c>ActivateWindow(MusicPlaylist)</c>
                </keyboard>
        </VisualisationPresetList>
        <SlideShow>
                <keyboard>
                        <play_pause>Pause</play_pause>
                        <m>ActivateWindow(PlayerControls)</m>
                        <o>CodecInfo</o>
                        <pageup>ZoomIn</pageup>
                        <pagedown>ZoomOut</pagedown>
                        <return>Rotate</return>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </SlideShow>
        <ScreenCalibration>
                <keyboard>
                        <return>NextCalibration</return>
                        <f1>ResetCalibration</f1>
                        <f3>NextResolution</f3>
                </keyboard>
        </ScreenCalibration>
        <VideoOSD>
                <keyboard>
                        <m>Back</m>
                        <c>ActivateWindow(VideoPlaylist)</c>
                        <f6>AudioDelay</f6>
                        <semicolon>AspectRatio</semicolon>
                        <o>CodecInfo</o>
                        <f1>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"ActivateWindow(Teletext)","ActivateWindow(SubtitleSearch)")</f1>
                        <f2>ActivateWindow(OSDVideoSettings)</f2>
                        <f3>ActivateWindow(OSDAudioSettings)</f3>
                        <f4>RunScript(special://xbmc/system/keymaps/tlbb/py_ispvr.py,"PlayerControl(Record)","ActivateWindow(VideoBookmarks)")</f4>
                </keyboard>
        </VideoOSD>
        <VideoMenu>
                <keyboard>
                        <play_pause>Select</play_pause>
                        <m>OSD</m>
                        <o>CodecInfo</o>
                        <f6>AudioDelay</f6>
                        <semicolon>AspectRatio</semicolon>
                        <f1>ActivateWindow(SubtitleSearch)</f1>
                        <f2>ActivateWindow(OSDVideoSettings)</f2>
                        <f3>ActivateWindow(OSDAudioSettings)</f3>
                        <f4>ActivateWindow(VideoBookmarks)</f4>
                        <backspace>Fullscreen</backspace>
                        <return>Select</return>
                        <period>SkipNext</period>
                        <comma>SkipPrevious</comma>
                </keyboard>
        </VideoMenu>
         <OSDVideoSettings>
                <keyboard>
                        <o>Info</o>
                        <f2>Close</f2>
                </keyboard>
        </OSDVideoSettings>
        <OSDAudioSettings>
                <keyboard>
                         <o>Info</o>
                        <f3>Close</f3>
                </keyboard>
        </OSDAudioSettings>
        <VideoBookmarks>
                <keyboard>
                        <f1>Delete</f1>
                        <f4>Close</f4>
                </keyboard>
        </VideoBookmarks>
        <Videos>
                <keyboard>
                        <f1>Container.NextViewMode</f1>
                        <f2>Info</f2>
                        <f3>SendClick(8)</f3>
                        <f4>SendClick(3)</f4>
                        <zero>ActivateWindow(VideoPlaylist)</zero>
                        <nine>ToggleWatched</nine>
                </keyboard>
        </Videos>
        <VideoPlaylist>
                <keyboard>
                        <zero>Back</zero>
                        <pageup>MoveItemUp</pageup>
                        <pagedown>MoveItemDown</pagedown>
                        <f1>Delete</f1>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </VideoPlaylist>
        <Pictures>
                <keyboard>
                        <f1>Container.NextViewMode</f1>
                        <f3>SendClick(7)</f3>
                                <f2>Info</f2>
                                <f4>ActivateWindow(Videos,Addons,return)</f4>
                                <o>Info</o>
                </keyboard>
        </Pictures>
        <ContextMenu>
                <keyboard>
                        <m>Back</m>
                </keyboard>
        </ContextMenu>
        <MusicInformation>
                <keyboard>
                        <o>Back</o>
                </keyboard>
        </MusicInformation>
        <MovieInformation>
                <keyboard>
                        <o>Back</o>
                </keyboard>
        </MovieInformation>
        <PictureInfo>
                <keyboard>
                        <play_pause>Pause</play_pause>
                        <pageup>NextPicture</pageup>
                        <pagedown>PreviousPicture</pagedown>
                        <o>Back</o>
                </keyboard>
        </PictureInfo>
        <Teletext>
                <keyboard>
                        <f1>Close</f1>
                </keyboard>
        </Teletext>
        <Favourites>
                <keyboard>
                        <pageup>MoveItemUp</pageup>
                        <pagedown>MoveItemDown</pagedown>
                        <f1>Delete</f1>
                        <f3>Close</f3>
                </keyboard>
        </Favourites>
        <NumericInput>
                <keyboard>
                        <backspace>Backspace</backspace>
                </keyboard>
        </NumericInput>
        <FullscreenLiveTV>
                <keyboard>
                        <m>OSD</m>
                        <left>StepBack</left>
                        <right>StepForward</right>
                        <up>ActivateWindow(PVROSDChannels)</up>
                        <down>ActivateWindow(PVROSDChannels)</down>
                        <pageup>ChannelUp</pageup>
                        <pagedown>ChannelDown</pagedown>
                        <f6>AudioDelay</f6>
                        <c>XBMC.ActivateWindowAndFocus(MyPVR, 31,0, 10,0)</c>
                        <semicolon>AspectRatio</semicolon>
                        <o>CodecInfo</o>
                        <f1>ActivateWindow(Teletext)</f1>
                        <f2>ActivateWindow(OSDVideoSettings)</f2>
                        <f3>ActivateWindow(OSDAudioSettings)</f3>
                        <f4>PlayerControl(Record)</f4>
                </keyboard>
        </FullscreenLiveTV>
        <PVROSDChannels>
                <keyboard>
                        <backspace>Close</backspace>
                        <return>RunScript(special://xbmc/system/keymaps/tlbb/py_select_close.py)</return>
                        <pageup>ChannelUp</pageup>
                        <pagedown>ChannelDown</pagedown>
                </keyboard>
        </PVROSDChannels>
        <PVROSDGuide>
                <keyboard>
                        <backspace>Close</backspace>
                        <o>Select</o>
                </keyboard>
        </PVROSDGuide>
        <PVRGuideInfo>
                <keyboard>
                        <backspace>Close</backspace>
                        <o>Close</o>
                </keyboard>
        </PVRGuideInfo>
        <FileBrowser>
                <keyboard>
                        <f1>Delete</f1>
                        <f2>Highlight</f2>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </FileBrowser>
        <ShutdownMenu>
                <keyboard>
                        <backspace>Close</backspace>
                </keyboard>
        </ShutdownMenu>
        <AddonInformation>
                <keyboard>
                        <o>Back</o>
                </keyboard>
        </AddonInformation>
        <AddonSettings>
                <keyboard>
                        <f1>Delete</f1>
                </keyboard>
        </AddonSettings>
        <Weather>
                <keyboard>
                        <f3>ActivateWindow(Favourites)</f3>
                        <f4>ActivateWindow(Videos,Addons,return)</f4>
                </keyboard>
        </Weather>
        <SliderDialog>
                <keyboard>
                        <f6>Back</f6>
                </keyboard>
        </SliderDialog>
        <window1111>
                <keyboard>
                        <!-- <f1>SendClick(81)</f1> --> <!-- NOT WORKING -->
                        <!-- <f1>Control.Message(81,click,1111)</f1> --> <!-- NOT WORKING -->
                        <f4>Control.Move(4001,1)</f4>
                        <f3>Control.Move(4001,-1)</f3>
                </keyboard>
        </window1111>
</keymap>
